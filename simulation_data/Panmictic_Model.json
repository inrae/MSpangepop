{
  "name": "Panmictic_Model",
  "description": "Single panmictic population model with parameter ranges",
  
  "simulation_params": {
    "fasta_gz": "simulation_data/small_test_genome.fa.gz",
    "chr_n": 1,
    "sv_distribution": {
      "SNP": 30,
      "DEL": 20,
      "INS": 20,
      "INV": 20,
      "DUP": 10
    }
  },
  
  "evolutionary_params": {
    "mutation_rate": 1e-7,
    "recombination_rate": 1e-8,
    "generation_time": 25
  },
  
  "populations": [
    {
      "id": "pop1",
      "initial_size": 5000
    }
  ],
  
  "samples": [
    {
      "population": "pop1",
      "sample_size": 3
    }
  ]
} 