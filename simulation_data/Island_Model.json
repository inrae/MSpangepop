{
  "name": "Island_Model",
  "description": "Island model with both fixed and range parameters",
  
  "simulation_params": {
    "fasta_gz": "simulation_data/small_test_genome.fa.gz",
    "chr_n": 1,
    "sv_distribution": {
      "SNP": 30,
      "DEL": 20,
      "INS": 15,
      "INV": 20,
      "DUP": 15
    }, 
    "max_sv": {
      "SNP": {"min": 50, "max": 200},
      "DEL": {"min": 10, "max": 50},
      "INS": {"min": 10, "max": 50},
      "INV": {"min": 5, "max": 30},
      "DUP": {"min": 5, "max": 30}
    },
    "sv_length_files": {
      "DEL": "simulation_data/size_distribDEL.tsv",
      "INS": "simulation_data/size_distribINS.tsv",
      "INV": "simulation_data/size_distribINV.tsv",
      "DUP": "simulation_data/size_distribDUP.tsv"
    },
    "seed": "Pangenome"
  },
  
  "evolutionary_params": {
    "mutation_rate": {"min": 5e-8, "max": 2e-7},
    "recombination_rate": 0.5e-8,
    "generation_time": 25
  },
  
  "populations": [
    { "id": "ISLAND_1", "initial_size": 1000 },
    { "id": "ISLAND_2", "initial_size": {"min": 800, "max": 1200} },
    { "id": "ISLAND_3", "initial_size": {"min": 800, "max": 1200} },
    { "id": "ISLAND_4", "initial_size": 1000 },
    { "id": "ISLAND_5", "initial_size": {"min": 1500, "max": 2000} }
  ],
  
  "samples": [
    { "population": "ISLAND_1", "sample_size": 2 },
    { "population": "ISLAND_2", "sample_size": 2 },
    { "population": "ISLAND_3", "sample_size": 2 },
    { "population": "ISLAND_4", "sample_size": 2 },
    { "population": "ISLAND_5", "sample_size": 2 }
  ],
  
  "demographic_events": [
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_1", "dest": "ISLAND_2", "rate": {"min": 5e-5, "max": 2e-4} },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_2", "dest": "ISLAND_1", "rate": {"min": 5e-5, "max": 2e-4} },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_2", "dest": "ISLAND_3", "rate": 1e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_3", "dest": "ISLAND_2", "rate": 1e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_3", "dest": "ISLAND_4", "rate": 1e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_4", "dest": "ISLAND_3", "rate": 1e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_4", "dest": "ISLAND_1", "rate": 1e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_1", "dest": "ISLAND_4", "rate": 1e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_1", "dest": "ISLAND_5", "rate": 2e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_5", "dest": "ISLAND_1", "rate": 2e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_2", "dest": "ISLAND_5", "rate": 2e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_5", "dest": "ISLAND_2", "rate": 2e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_3", "dest": "ISLAND_5", "rate": 2e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_5", "dest": "ISLAND_3", "rate": 2e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_4", "dest": "ISLAND_5", "rate": 2e-4 },
    { "type": "add_migration_rate_change", "time": 2000, "source": "ISLAND_5", "dest": "ISLAND_4", "rate": 2e-4 }
  ]
}